{
  "name": "@snowball-tech/fractal-react",
  "version": "1.0.0",
  "description": "React component library based on Fractal - Snowball's design system",
  "private": true,
  "author": "Snowball Engineering Team <engineering@snowball.xyz> (https://github.com/orgs/snowball-tech)",
  "contributors": [
    "Clément PRÉVOT <clement@snowball.xyz> (https://github.com/clementprevot)"
  ],
  "license": "MIT",
  "keywords": [
    "snowball",
    "design-system",
    "fractal",
    "react",
    "styled-components"
  ],
  "homepage": "https://github.com/snowball-tech/fractal/tree/main/packages/react",
  "repository": {
    "type": "git",
    "url": "https://github.com/snowball-tech/fractal.git",
    "directory": "packages/react"
  },
  "bugs": "https://github.com/snowball-tech/fractal/issues",
  "packageManager": "yarn@3.6.0",
  "engines": {
    "yarn": "^3.0.0",
    "node": "^18.0.0"
  },
  "type": "module",
  "source": "src/index.ts",
  "module": "dist/fractal-react.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fractal-react.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "prebuild:chromatic": "yarn run -T turbo run update:fonts && yarn run clean:storybook",
    "build:chromatic": "yarn run prebuild:chromatic && yarn run -T chromatic --build-script-name build:chromatic --exit-zero-on-changes",
    "prebuild:storybook": "yarn run clean:storybook",
    "build:storybook": "yarn run prebuild:storybook && storybook build",
    "dev": "storybook dev -p 6007",
    "dev:with-fonts": "yarn run -T turbo run update:fonts && yarn run dev",
    "clean:storybook": "rm -Rf storybook-static",
    "format": "yarn run -T prettier --ignore-path ../../.prettierignore --cache --check .",
    "format:fix": "yarn run -T prettier --ignore-path ../../.prettierignore --cache --write .",
    "lint": "yarn run lint:scripts && yarn run lint:styles",
    "lint:fix": "yarn run lint:scripts:fix && yarn run lint:styles:fix",
    "lint:scripts": "yarn run -T eslint --cache .",
    "lint:scripts:fix": "yarn run -T eslint --cache --fix .",
    "lint:styles": "yarn run -T stylelint --cache \"./**/*.{html,css,jsx,tsx}\" \"!storybook-static\"",
    "lint:styles:fix": "yarn run -T stylelint --cache --fix \"./**/*.{html,css,jsx,tsx}\" \"!storybook-static\"",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\"",
    "types:check": "tsc --noEmit",
    "preupdate:fonts": "rm -Rf assets/fonts 2>&1 || true",
    "update:fonts": "yarn run preupdate:fonts && ../../scripts/update-fonts.sh ./assets/"
  },
  "devDependencies": {
    "@snowball-tech/eslint-config": "^3.1.0",
    "@storybook/addon-a11y": "^7.0.22",
    "@storybook/addon-essentials": "^7.0.22",
    "@storybook/addon-interactions": "^7.0.22",
    "@storybook/addon-links": "^7.0.22",
    "@storybook/addon-styling": "^1.3.0",
    "@storybook/addons": "^7.0.22",
    "@storybook/blocks": "^7.0.22",
    "@storybook/components": "^7.0.22",
    "@storybook/manager-api": "^7.0.22",
    "@storybook/react": "^7.0.22",
    "@storybook/react-vite": "^7.0.22",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^7.0.22",
    "@types/eslint": "^8.40.2",
    "@types/lodash": "^4.14.195",
    "@types/prettier": "^2.7.3",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.13",
    "@types/react-dom": "^18.2.6",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.43.0",
    "eslint-plugin-react-refresh": "^0.4.1",
    "eslint-plugin-storybook": "^0.6.12",
    "install": "^0.13.0",
    "json": "^11.0.0",
    "npm": "^9.7.1",
    "postcss": "^8.4.24",
    "postcss-styled-syntax": "^0.4.0",
    "prettier": "^2.8.8",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.22",
    "storybook-addon-mock": "^4.0.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-lib": "^1.2.2"
  },
  "dependencies": {
    "@iconscout/react-unicons": "^2.0.2",
    "@snowball-tech/design-tokens": "^1.6.1",
    "@snowball-tech/fractal": "workspace:^",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.0-rc.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.0.0-rc.3"
  }
}
